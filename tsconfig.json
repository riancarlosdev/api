{
  "compilerOptions": {
    "target": "es2016",
    "module": "ES6",
    "rootDir": "./app",
    "moduleResolution": "node",
    "outDir": "./dist",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
  },
}
